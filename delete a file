#include <stdio.h>
#include <stdlib.h>

int main() {
    char filename[100];

    
    printf("Enter the name of the file to delete: ");
    scanf("%s", filename);

    
    if (remove(filename) == 0) {
        printf("File %s deleted successfully.\n", filename);
    } else {
        printf("Could not delete the file %s. File may not exist.\n", filename);
    }

    return 0
